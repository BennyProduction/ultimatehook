local a=game.ReplicatedStorage;local b=loadstring(game:HttpGet("https://raw.githubusercontent.com/vozoid/ui-libraries/main/drawing/void/source.lua"))()local c=game.Players.LocalPlayer;local d=b:Watermark("ultimatehook | v2.10 | Everyday Car Driving | "..c.Name.." ")local e=b:Load({Name="Ultimatehook",SizeX=350,SizeY=350,Theme="Midnight",Extension="lua",Folder="Ultimateh00k"})local f=e:Tab("General")local g=f:Section({Name="Buttons",Side="Left"})local h=f:Section({Name="Toggles",Side="Right"})local i=f:Section({Name="Sliders",Side="Down"})g:Button({Name="Gamepass cars",Callback=function()while task.wait(.5)do game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Frame["Le Verare GAMEPASS"]:Destroy()game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Frame["RST9 GAMEPASS"]:Destroy()game:GetService("ReplicatedStorage")["Le_Verare"].Parent=game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Frame;game:GetService("ReplicatedStorage")["RST9"].Parent=game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Frame end end})local j=e:Tab("Configuration")local k=j:Section{Name="Configs",Side="Left"}local l=h:Toggle{Name="Autofarm",Flag="Autofarm",Callback=function(m)AutoFarm=m;if m then while wait()do game:GetService("ReplicatedStorage").GetCash:FireServer()if AutoFarm==false then break end end end end}local n=i:Slider{Name="FOV",Text="",Min=70,Max=120,Float=0.1,Flag="FOV Slider",Callback=function(o)workspace.CurrentCamera.FieldOfView=o end}local p=k:Dropdown{Name="Configs",Content=b:GetConfigs(),Flag="Config Dropdown"}local q=k:Button{Name="Load Config",Callback=function()b:LoadConfig(b.flags["Config Dropdown"])end}local r=k:Button{Name="Delete Config",Callback=function()b:DeleteConfig(b.flags["Config Dropdown"])p:Refresh(b:GetConfigs())end}local s=k:Box{Name="Config Name",Placeholder="Config Name",Flag="Config Name"}local t=k:Button{Name="Save Config",Callback=function()b:SaveConfig(b.flags["Config Dropdown"]or b.flags["Config Name"])p:Refresh(b:GetConfigs())end}local u=j:Section{Name="UI Toggle Keybind",Side="Left"}u:Keybind{Name="UI Toggle",Flag="UI Toggle",Default=Enum.KeyCode.RightShift,Blacklist={Enum.UserInputType.MouseButton1,Enum.UserInputType.MouseButton2,Enum.UserInputType.MouseButton3},Callback=function(v,w)if not w then b:Close()end end}local x=j:Section{Name="Theme",Side="Left"}local y={}local z=x:Dropdown{Name="Theme",Default=b.currenttheme,Content=b:GetThemes(),Flag="Theme Dropdown",Callback=function(A)if A then b:SetTheme(A)for A,B in next,y do B:Set(b.theme[A])end end end}local C=x:Box{Name="Custom Theme Name",Placeholder="Custom Theme",Flag="Custom Theme"}x:Button{Name="Save Custom Theme",Callback=function()if b:SaveCustomTheme(b.flags["Custom Theme"])then z:Refresh(b:GetThemes())z:Set(b.flags["Custom Theme"])C:Set("")end end}local D=j:Section{Name="Custom Theme",Side="Right"}y["Accent"]=D:ColorPicker{Name="Accent",Default=b.theme["Accent"],Flag="Accent",Callback=function(E)b:ChangeThemeOption("Accent",E)end}y["Window Background"]=D:ColorPicker{Name="Window Background",Default=b.theme["Window Background"],Flag="Window Background",Callback=function(E)b:ChangeThemeOption("Window Background",E)end}y["Window Border"]=D:ColorPicker{Name="Window Border",Default=b.theme["Window Border"],Flag="Window Border",Callback=function(E)b:ChangeThemeOption("Window Border",E)end}y["Tab Background"]=D:ColorPicker{Name="Tab Background",Default=b.theme["Tab Background"],Flag="Tab Background",Callback=function(E)b:ChangeThemeOption("Tab Background",E)end}y["Tab Border"]=D:ColorPicker{Name="Tab Border",Default=b.theme["Tab Border"],Flag="Tab Border",Callback=function(E)b:ChangeThemeOption("Tab Border",E)end}y["Tab Toggle Background"]=D:ColorPicker{Name="Tab Toggle Background",Default=b.theme["Tab Toggle Background"],Flag="Tab Toggle Background",Callback=function(E)b:ChangeThemeOption("Tab Toggle Background",E)end}y["Section Background"]=D:ColorPicker{Name="Section Background",Default=b.theme["Section Background"],Flag="